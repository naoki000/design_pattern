<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.designpattern">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DesignPatternMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".iterator.IteratorPatternActivity"/>
        <activity android:name=".adapter.AdapterPatternActivity"/>
        <activity android:name=".templatemethod.TemplateMethodPatternActivity"/>
        <activity android:name=".factorymethod.FactoryMethodPatternActivity"/>
        <activity android:name=".singleton.SingletonPatternActivity"/>
        <activity android:name=".prototype.PrototypePatternActivity"/>
        <activity android:name=".builder.BuilderPatternActivity"/>
        <activity android:name=".abstractfactory.AbstractFactoryPatternActivity"/>
        <activity android:name=".bridge.BridgePatternActivity"/>
        <activity android:name=".strategy.StrategyPatternActivity"/>
        <activity android:name=".composite.CompositePatternActivity"/>
        <activity android:name=".decorator.DecoratorPatternActivity"/>
        <activity android:name=".visitor.VisitorPatternActivity"/>
        <activity android:name=".chainofresponsibility.ChainOfResponsibilityPatternActivity"/>
        <activity android:name=".facade.FacadePatternActivity"/>
        <activity android:name=".madiator.MediatorPatternActivity"/>
        <activity android:name=".observer.ObserverPatternActivity"/>
        <activity android:name=".memento.MementoPatternActivity"/>
        <activity android:name=".state.StatePatternActivity"/>

    </application>

</manifest>